<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.IntentImplicitoActivity">

    <!-- 17-Junio-2023 Creando un boton
    @+id/btRegister, asignar id para identificar el botÃ³n-->

    <Button

        android:id="@+id/btSend"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:backgroundTint="#1CCBD9"
        android:text="Enviar"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btUrl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Url"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btSend" />

</androidx.constraintlayout.widget.ConstraintLayout>